{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EAS_ENABLE_EXPO_DOCTOR": "0",
        "EXPO_PUBLIC_APP_ENV": "production"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "development"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6754253217"
      }
    }
  }
}